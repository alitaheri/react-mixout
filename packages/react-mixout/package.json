  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alitaheri/react-mixout/issues"
  },
  "homepage": "https://github.com/alitaheri/react-mixout#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "react": "^15.0.1",
    "react-addons-test-utils": "^15.0.1",
    "typescript": "^1.8.10",
    "typings": "^0.7.12"
  },
  "peerDependencies": {
    "react": ">=0.14.0"
  }
